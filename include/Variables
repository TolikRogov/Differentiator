struct Variable {
	VarNum num;
	const char* symbol;
	Number_t value;
};

static Variable var_name_table[AMOUNT_OF_VARIABLES] = {};
var_name_table[VAR_X] 	= 	{ .num = VAR_X, 	.symbol = "x", 	.value = 0 };
var_name_table[VAR_Y] 	= 	{ .num = VAR_Y, 	.symbol = "y", 	.value = 0 };
var_name_table[VAR_Z] 	= 	{ .num = VAR_Z, 	.symbol = "z", 	.value = 0 };
